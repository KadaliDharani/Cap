
insert into a binary seach tree:

class Solution {
    public TreeNode insertIntoBST(TreeNode root, int val) {
        TreeNode t=new TreeNode(val);
         if(root==null){
            return t;
        }
        TreeNode head=root;
        TreeNode p=null;
        while(head!=null){
            p=head;
            if(val>head.val){
                 head=head.right;
            }
            else{
                head=head.left;            }
        }
        if(val<p.val){
            p.left=t;
        }
        else{
            p.right=t;
        }
        return root;
    }
}
